{
  "governedRegions": ${governed_regions},
  "organizationStructure": {
    "security": {
      "name": "Security"
    },
    "sandbox": {
      "name": "Sandbox"
    }
  },
  "centralizedLogging": {
    "accountId": "${logging_account_id}",
    "configurations": {
      "loggingBucket": {
        "retentionDays": 913
      },
      "accessLoggingBucket": {
        "retentionDays": 913
      },
      "kmsKeyArn": "${kms_key_arn}"
    },
    "enabled": true
  },
  "securityRoles": {
    "accountId": "${security_account_id}"
  },
  "accessManagement": {
    "enabled": ${access_management_enabled}
  }
}